@model IEnumerable<Ads.Models.PuntajeUserAd>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Puntaje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAdNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsersNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Puntaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAdNavigation.IdAd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsersNavigation.IdUsers)
            </td>
            <td>
